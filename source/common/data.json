{
    "filetypes": [
        ".md",
        ".rmd",
        ".markdown",
        ".txt",
        ".tex",
        ".mdx"
    ],
    "ignoreDirs": [
        ".+?\\.app",
        ".+?\\.textbundle"
    ],
    "import_files": [
        [ "Markdown",                          "md", "markdown", "rmd", "mdx" ],
        [ "DocBook",                           "docbook" ],
        [ "Microsoft Word",                    "docx", "doc" ],
        [ "EPUB",                              "epub" ],
        [ "Haddock",                           "haddock", "hs" ],
        [ "Hypertext Markup Language",         "html", "htm" ],
        [ "LaTeX",                             "tex", "latex" ],
        [ "Muse",                              "muse" ],
        [ "Open Document Text",                "odt" ],
        [ "Outline Processor Markup Language", "opml" ],
        [ "Emacs .org file",                   "org" ],
        [ "reStructured Text",                 "rst" ],
        [ "Rich Text Format",                  "rtf" ],
        [ "txt2tags",                          "t2t" ],
        [ "TextBundle",                        "textbundle" ],
        [ "TextPack (Compressed TextBundle)",  "textpack" ],
        [ "Textile",                           "textile" ],
        [ "Wiki Markup",                       "wiki" ],
        [ "MAN pages (roff)",                  "man" ],
        [ "MAN pages (ms)",                    "ms" ],
        [ "FictionBook2",                      "fb2" ],
        [ "Jupyter Notebook",                  "ipynb" ],
        [ "Jira Markup",                       "jira" ]
    ]
}
