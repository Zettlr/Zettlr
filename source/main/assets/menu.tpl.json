{
  "app": {
    "label": "CUck",
    "submenu": [
      { "label": "menu.about", "role": "about" },
      { "mixin": "darwin_prefs" },
      { "type": "separator" },
      { "label": "menu.services", "role": "services", "submenu": [] },
      { "type": "separator" },
      { "label": "menu.hide", "role": "hide" },
      { "label": "menu.hide_others", "role": "hideothers" },
      { "label": "menu.unhide", "role": "unhide" }
    ]
  },
  "file": {
    "label": "menu.labels.file",
    "submenu": [
    {
      "label": "menu.new_file",
      "accelerator": "CmdOrCtrl+N",
      "command": "file-new"
    },{
      "label": "menu.new_dir",
      "accelerator": "CmdOrCtrl+Shift+N",
      "command": "dir-new"
    },
    { "type": "separator" },
    {
      "label": "menu.open",
      "accelerator": "CmdOrCtrl+O",
      "command": "dir-open"
    }, {
      "label": "menu.save",
      "accelerator": "CmdOrCtrl+S",
      "command": "file-save"
    }, {
      "label": "menu.recent_docs",
      "id": "recent-docs"
    },
    { "type": "separator" },
    { "label": "menu.import_files", "command": "import-files" },
    {
      "label": "menu.export",
      "accelerator": "CmdOrCtrl+E",
      "command": "export"
    },
    {
      "label": "menu.print",
      "accelerator": "CmdOrCtrl+P",
      "command": "print"
    },
    { "type": "separator" },
    { "label": "menu.import_lang_file", "command": "import-lang-file" },
    { "label": "menu.import_dict_file", "id": "openDictData" },
    { "type": "separator" },
    {
      "label": "menu.rename_file",
      "accelerator": "CmdOrCtrl+R",
      "command": "file-rename"
    }, {
      "label": "menu.rename_dir",
      "accelerator": "CmdOrCtrl+Shift+R",
      "command": "dir-rename"
    },
    { "type": "separator" },
    {
      "label": "menu.delete_file",
      "accelerator": { "darwin": "Cmd+Backspace", "other": "Delete" },
      "command": "file-delete"
    }, {
      "label": "menu.delete_dir",
      "accelerator": { "darwin": "Cmd+Shift+Backspace", "other": "Ctrl+Delete" },
      "command": "dir-delete"
    },
    { "mixin": "other_prefs" }
  ]},
  "edit": {
    "label": "menu.labels.edit",
    "submenu": [
    { "label": "menu.undo", "role": "undo" },
    { "label": "menu.redo", "role": "redo" },
    { "type": "separator" },
    { "label": "menu.cut", "role": "cut" },
    { "label": "menu.copy", "role": "copy" },
    {
      "label": "menu.copy_html",
      "accelerator": "CmdOrCtrl+Alt+C",
      "command": "copy-as-html"
    },
    { "label": "menu.paste", "role": "paste" },
    {
      "label": "menu.paste_plain",
      "accelerator": "CmdOrCtrl+Shift+V",
      "command": "paste-as-plain"
    },
    { "label": "menu.select_all", "role": "selectall" },
    { "type": "separator" },
    {
      "label": "menu.find_file",
      "accelerator": "CmdOrCtrl+F",
      "command": "file-find"
    }, {
      "label": "menu.find_dir",
      "accelerator": "CmdOrCtrl+Shift+F",
      "command": "dir-find"
    },
    { "type": "separator" },
    {
      "label": "menu.generate_id",
      "accelerator": "CmdOrCtrl+L",
      "command": "insert-id"
    },
    { "mixin": "darwin_speech" }
  ]},
  "view": {
    "label": "menu.labels.view",
    "submenu": [
    {
      "label": "menu.toggle_theme",
      "accelerator": "CmdOrCtrl+Alt+L",
      "type": "checkbox",
      "checked": "darkTheme",
      "command": "toggle-theme"
    }, {
      "label": "menu.toggle_file_meta",
      "accelerator": "CmdOrCtrl+Alt+S",
      "type": "checkbox",
      "checked": "fileMeta",
      "command": "toggle-file-meta"
    }, {
      "label": "menu.toggle_distraction_free",
      "accelerator": "CmdOrCtrl+J",
      "type": "checkbox",
      "checked": "null",
      "command": "toggle-distraction-free"
    },
    { "type": "separator" },
    {
      "label": "menu.toggle_sidebar",
      "accelerator": "CmdOrCtrl+!",
      "command": "toggle-sidebar"
    }, {
      "label": "menu.toggle_attachments",
      "accelerator": "CmdOrCtrl+?",
      "command": "toggle-attachments"
    },
    { "type": "separator" },
    {
      "label": "menu.reset_zoom",
      "accelerator": "CmdOrCtrl+0",
      "command": "zoom-reset"
    }, {
      "label": "menu.zoom_in",
      "accelerator": "CmdOrCtrl+Plus",
      "command": "zoom-in"
    }, {
      "label": "menu.zoom_out",
      "accelerator": "CmdOrCtrl+-",
      "command": "zoom-out"
    },
    { "type": "separator" },
    { "label": "menu.toggle_fullscreen", "role": "togglefullscreen" }
  ]},
  "window": {
    "label": "menu.labels.window",
    "role": "window",
    "submenu": [
    {
      "label": "menu.win_minimize",
      "accelerator": "CmdOrCtrl+M",
      "role": "minimize"
    }, {
      "label": "menu.win_close",
      "accelerator": "CmdOrCtrl+W",
      "role": "close"
    },
    { "label": "menu.all_front", "role": "front" }
  ]},
  "help": {
    "label": "menu.labels.help",
    "role": "help",
    "submenu": [
    { "label": "menu.about", "command": "display-about" },
    {
      "label": "menu.donate",
      "target": "https://patreon.com/Gettlr"
    },
    {
      "label": "menu.learn_more",
      "target": "https://www.Gettlr.com/"
    }, {
      "label": "menu.pandoc",
      "target": "https://github.com/jgm/pandoc/releases/latest"
    }, {
      "label": "menu.latex",
      "target": "https://www.latex-project.org/get/#tex-distributions"
    }, {
      "label": "menu.docs",
      "accelerator": "F1",
      "target": "https://docs.Gettlr.com/"
    },
    { "label": "menu.update", "command": "update-check" }
  ]},
  "debug": {
    "label": "menu.labels.debug",
    "submenu": [
      {
        "label": "menu.inspect_clipboard",
        "command": "inspect-clipboard"
      },
      {
        "label": "menu.reload",
        "accelerator": "F5",
        "id": "reloadWindow"
      }, {
        "label": "menu.toggle_devtools",
        "accelerator": { "darwin": "Alt+Command+I", "other": "Ctrl+Shift+I" },
        "id": "toggleDevTools"
      }, {
        "label": "Open Logs",
        "accelerator": "CmdOrCtrl+Alt+Shift+L",
        "id": "openLogViewer"
      }
  ]},
  "mixin_preferences": {
    "submenu": [
      {
        "label": "menu.preferences",
        "accelerator": "CmdOrCtrl+,",
        "command": "open-preferences"
      }, {
        "label": "menu.pdf_preferences",
        "accelerator": "CmdOrCtrl+Alt+,",
        "command": "open-pdf-preferences"
      },
      { "label": "menu.tags", "command": "open-tags-preferences" },
      { "label": "menu.custom_css", "command": "open-custom-css" }
    ]
  },
  "mixin_speech": {
    "submenu": [
      { "type": "separator" },
      {
        "label": "menu.speech",
        "submenu": [
          { "label": "menu.start_speaking", "role": "startspeaking" },
          { "label": "menu.stop_speaking", "role": "stopspeaking" }
        ]
      }
    ]
  }
}
