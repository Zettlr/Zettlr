    "ky": "^0.33.3",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "md5": "^2.3.0",
    "mermaid": "^9.x.x",
    "minimatch": "^8.0.3",
    "nodehun": "^3.0.2",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-lint": "^9.1.1",
    "remark-lint-blockquote-indentation": "^3.1.1",
    "remark-lint-checkbox-character-style": "^4.1.1",
