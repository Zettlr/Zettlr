    "got": "^13.0.0",
    "katex": "^0.16.9",
    "ky": "^0.33.3",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "md5": "^2.3.0",
    "mermaid": "^10.2.3",
    "minimatch": "^9.0.3",
    "nodehun": "^3.0.2",
    "pinia": "^2.1.7",
    "rehype-parse": "^9.0.0",
