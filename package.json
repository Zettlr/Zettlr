    "unified": "^10.1.2",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-virtual-scroller": "^2.0.0-beta.7",
    "vuex": "^4.1.0",
    "yaml": "^2.2.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "resolutions": {
    "@codemirror/language": "^6.6.0",
